# Server Configuration
# PORT is hardcoded to 3587 (DayTradersPro dedicated port)
NODE_ENV=development

# ProjectX API Configuration
PROJECTX_API_URL=https://api.topstepx.com
PROJECTX_WEBSOCKET_URL=wss://rtc.topstepx.com
PROJECTX_API_KEY=your_api_key_here
PROJECTX_API_SECRET=your_api_secret_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Data Storage
DATA_PATH=~/Desktop/DayTradersPro
ALGORITHMS_PATH=~/Desktop/DayTradersPro/algorithms
HISTORICAL_DATA_PATH=~/Desktop/DayTradersPro/historical

# Logging
LOG_LEVEL=info
